{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "python3 clear_cache.py && node server.mjs",
    "healthcheckPath": "/models",
    "restartPolicyType": "ON_FAILURE"
  }
}
